{
  "name": "codechain-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/reactstrap": "^6.0.3",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.2",
    "codechain-sdk": "^0.4.0-rc3",
    "deepmerge": "^2.1.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.0",
    "react-chartjs-2": "^2.7.4",
    "react-color": "^2.14.1",
    "react-confirm-alert": "^2.0.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.5.0",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "react-toastify": "^4.3.2",
    "react-vis-force": "^0.3.1",
    "reactstrap": "^6.4.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "rpc-websockets": "^4.3.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -p tsconfig.json && prettier 'src/**/*.{ts,tsx,scss,json,html,js,jsx}' -l",
    "fmt": "tslint -p tsconfig.json --fix && prettier 'src/**/*.{ts,tsx,scss,json,html,js,jsx}' --write"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.34",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-color": "^2.13.6",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-datepicker": "^1.1.7",
    "@types/react-dom": "^16.0.7",
    "@types/react-modal": "^3.2.1",
    "@types/react-redux": "^6.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-logger": "^3.0.6",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  }
}
